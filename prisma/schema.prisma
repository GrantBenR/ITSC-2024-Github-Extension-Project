model User {
  id    Int     @id @default(autoincrement())
  UserName String  @unique
  timers Timer[]
}
model Issue {
  id    Int     @id @default(autoincrement())
  issueName String  @unique
  timers Timer[]
}
model TimerPeriod {
    id    Int     @id @default(autoincrement())
    startDate String
    endDate String
}
model Timer {
  id    Int     @id @default(autoincrement())
  issue Issue    @relation(fields: [issueId], references: [id])
  user User    @relation(fields: [userId], references: [id])
  timerPeriods TimerPeriod[]
}